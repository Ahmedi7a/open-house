<%- include('../partials/head') %> <br>
    <div class="container" >
      <div class="houses-con">

      
        <h1>Here's all the listings</h1><br>
            <% listings.forEach((listing) => { %>

                <div class="card mt-2" style="width: 18rem;">
                    <img src="<%= listing.imgUrl %>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title">Price: $<%= listing.price %></h5>
                      <p class="card-text"><%= listing.streetAddress %></p>
                      <a href="/listings/<%=listing._id %>" class="btn btn-primary">View Details</a>
                    </div>
                  </div>

            <% }) %>  
          </div>
    </div>

<%- include('../partials/footer') %>